<mvc:View controllerName="Ejercicio19.Ejercicio19.controller.Main" xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table" xmlns="sap.m">
	<Page title="{i18n>titleSecondView}" showNavButton="true" navButtonPress="onNavBack"> 
        <Panel  width="auto" class="sapUiResponsiveMargin" id="panelTablaId" expandable="true" expanded="true">
             <form:SimpleForm
                    title="{i18n>datosProducto}"
                    minWidth="1024"
                    editable="false"
                    layout="ResponsiveGridLayout">
                <form:content>
                    <Label text="{i18n>customerId}"/>
                    <Text text="{modelProducto>/CustomerID}"/>
                    <Label text="{i18n>employeeId}"/>
                    <Text text="{modelProducto>/EmployeeID}"/>
                    <Label text="{i18n>ordenId}"/>
                    <Text text="{modelProducto>/OrderID}"/>
                    <Label text="{i18n>shipAdress}"/>
                    <Text text="{modelProducto>/ShipAddress}"/>
                    <Label text="{i18n>shipCity}"/>
                    <Text text="{modelProducto>/ShipCity}"/>
                    <Label text="{i18n>ShipCountry}"/>
                    <Text text="{modelProducto>/ShipCountry}"/>
                    <Label text="{i18n>shipRegion}"/>
                    <Text text="{modelProducto>/ShipRegion}"/>
                    <Label text="{i18n>shipName}"/>
                    <Text text="{modelProducto>/ShipName}"/>
                    <Label text="{i18n>shipPostalCode}"/>
                    <Text text="{modelProducto>/ShipPostalCode}"/>
                </form:content>
            </form:SimpleForm>
        </Panel>
    </Page>
</mvc:View>
